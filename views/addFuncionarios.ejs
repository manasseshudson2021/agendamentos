                                        <div class="form-line">
                                            <input type="text" class="form-control" placeholder="Bairro" id="bairro" name="bairro" />
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <label for="exampleInputEmail1">Cidade</label>
                                    <div class="form-group">
                                        <div class="form-line">
